#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/dist/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/dist/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules:/mnt/e/projects/nextjs/omw/ui-topia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/dist/bin-esm.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-node@10.9.1_@types+node@20.14.2_typescript@5.4.5/node_modules/ts-node/dist/bin-esm.js" "$@"
fi
